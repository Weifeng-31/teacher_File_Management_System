<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.cqy.entity.UserMenu" table="menu" schema="admin">
        <id name="id" >
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property name="roleId" column="role_id"/>
        <property name="roleName" column="role_name"/>
        <property name="menuId" column="menuId"/>
        <property name="menuName" column="menuName"/>
        <property name="menuHref" column="menuHref"/>
        <property name="menuIcon" column="menuIcon"/>
        <property name="parentMenuId" column="parentMenuId"/>
    </class>
</hibernate-mapping>